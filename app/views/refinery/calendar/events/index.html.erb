<% content_for :body do %>

    <%#= render @events %>



 <section id="events" class="container">
  <%= month_calendar(attribute: :from, events: @events) do |date, events| %>
  <%= date.strftime("%d") %>

  <% events.each do |event| %>
    <div>
     <b><%= link_to event.title, refinery.calendar_event_path(event) %></b>
     <p><%= event.excerpt %></p>

      <%#= meeting.title %>
    </div>
  <% end %>
<% end %>

</section>

<% end %>

<% content_for :stylesheets, stylesheet_link_tag('events') %>

<%= render '/refinery/content_page' %>
